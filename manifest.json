{
	"name": "web2py dev mode for Chrome",
	"version": "1.2",
	"description": "Automatically opens local web2py error tickets in iframe",
	"permissions": [
		"tabs", "http://127.0.0.1:8000/*", "http://localhost:8000/*"
	],
	"content_scripts": [
		{
		"matches":  ["http://127.0.0.1/*","http://localhost/*"],
		"js": ["contentscript.js"],
		"run_at": "document_idle"
		}
	]
}
